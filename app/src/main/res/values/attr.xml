<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Use unique name to avoid aapt2 attr duplicate resource error-->

    <!--This is to prevent the error duplicate value for resource 'attr/mode' in host container project that would use this library-->
    <declare-styleable name="WeCardCustomTextView">
        <attr name="weTvTypeface" format="enum">
            <enum name="we_regular" value="0" />
            <enum name="we_bold" value="1" />
        </attr>
        <attr name="we_ctvTxtColor" format="color" />
        <attr name="we_ctvSelectedTxtColor" format="color" />
    </declare-styleable>

</resources>